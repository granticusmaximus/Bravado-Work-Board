@using Bravado.Models
@model Bravado.Models.Board
@{
    ViewData["Title"] = "Edit Board";
}

<div class="container">
    <form method="post">
        <input asp-for="Title" placeholder="Title" Model.Title required>
        <input hidden asp-for="Id" Model.Id>
        <button asp-route-id="Model.Id" class="btn center-align" type="submit">
            Update <i class="fa fa-cog" aria-hidden="true"></i>
        </button>
    </form>
</div>


<script>$(document).ready(function () {
        M.updateTextFields();
    });</script>