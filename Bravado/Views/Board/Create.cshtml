@model Bravado.ViewModel.NewBoard;
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <h2>Add a new Board</h2>
    <form method="post" asp-action="Create" asp-controller="Board">
        <label>Title</label>
        <input type="text" asp-for="Title" />
        <input type="submit" class="btn" />
    </form>
</div>

